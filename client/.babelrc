{
  "presets": [
    "es2015",
    "stage-0",
    "react"
  ],
  "plugins": [
    [
      "module-alias",
      [
        {
          "src": "./app/cable",
          "expose": "cable"
        },
        {
          "src": "./app",
          "expose": "app"
        },
        {
          "src": "./test",
          "expose": "test"
        },
        {
          "src": "./app/helpers",
          "expose": "helpers"
        },
        {
          "src": "./app/components",
          "expose": "components"
        },
        {
          "src": "./app/proptypes/schemas",
          "expose": "schemas"
        },
        {
          "src": "./app/proptypes",
          "expose": "proptypes"
        },
        {
          "src": "./app/sources",
          "expose": "sources"
        },
        {
          "src": "./app/models",
          "expose": "models"
        },
        {
          "src": "./app/texts",
          "expose": "texts"
        },
        {
          "src": "npm:lodash",
          "expose": "underscore"
        }
      ]
    ]
  ]
}
