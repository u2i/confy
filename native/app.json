{
  "expo": {
    "name": "Confy",
    "description": "Confy Client for mobile",
    "slug": "confy-native",
    "privacy": "unlisted",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.6",
    "orientation": "landscape",
    "icon": "./assets/u2i_logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.u2i.confy"
    }
  }
}
